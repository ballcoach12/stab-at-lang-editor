{
  "name": "wrapper",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean": "shx rm -rf ./dist *.tsbuildinfo",
    "compile": "tsc --build tsconfig.json",
    "dev": "vite",
    "dev:debug": "vite --debug --force",
    "build": "npm run clean && npm run build:vite",
    "build:vite": "tsc --build tsconfig.json && vite build",
    "preview": "vite preview",
    "postinstall": "monaco-treemending"
  },
  "dependencies": {
    "monaco-editor-workers": "^0.43.0",
    "monaco-editor-wrapper": "^3.0.1",
    "vite-plugin-node-polyfills": "^0.14.1",
    "vscode-languageclient": "^8.1.0"
  },
  "devDependencies": {
    "@types/node": "~18.17.5",
    "@types/shelljs": "~0.8.12",
    "@types/vscode": "~1.81.0",
    "@typescript-eslint/eslint-plugin": "~6.6.0",
    "@typescript-eslint/parser": "~6.6.0",
    "@vitest/browser": "~0.34.3",
    "editorconfig": "~2.0.0",
    "eslint": "~8.48.0",
    "path-browserify": "~1.0.1",
    "shelljs": "~0.8.5",
    "shx": "~0.3.4",
    "ts-node": "~10.9.1",
    "tslib": "~2.6.2",
    "typescript": "~5.1.6",
    "vite": "^4.4.9",
    "vite-node": "~0.34.3",
    "vitest": "~0.34.3",
    "webdriverio": "~8.16.4"
  }
}
